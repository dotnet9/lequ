@model IEnumerable<Lequ.Model.Models.BlogTag>
<div class="sidebar-box">
    <h3 class="heading">Tags</h3>
    <ul class="tags">

        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <li><a href="#">@item.Tag?.Name</a></li>
            }
        }
    </ul>
</div>