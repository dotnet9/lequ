@model IEnumerable<Lequ.Model.BlogCategory>
<div class="sidebar-box">
    <h3 class="heading">Categories</h3>
    <ul class="tags">
        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <li><a href="/Blog/ListByCategory?categoryID=@item.Category?.ID">@item.Category?.Name</a></li>
            }
        }
    </ul>
</div>